{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch index.html",
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"file": "${workspaceFolder}/index.html"
		},
		{
			"name": "Launch localhost",
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"url": "http://localhost/index.html",
			"webRoot": "${workspaceFolder}"
		},
		{
			"name": "Attach",
			"type": "firefox",
			"request": "attach"
		}
	]
}